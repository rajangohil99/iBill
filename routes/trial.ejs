<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1><%= title =%></h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>email</th>
                <th>password</th>
               
            </tr>
        </thead>

        <% records.forEach (function(a) { %> 

      
        <tr>
            <td><%= a.name =%></td>
            <td><%= a.email =%></td>
            <td><%= a.password =%></td>
         
        
        </tr>
        <% }) %>
    </table>

   
</body>
</html>