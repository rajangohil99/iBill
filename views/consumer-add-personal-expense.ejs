            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <!-- Sidebar navigation-->
                <nav class="sidebar-nav">
                    <ul id="sidebarnav">
                        <!-- User Profile-->
                        <li class="sidebar-item"> <a class="sidebar-link waves-effect waves-dark sidebar-link"
                            href="index.html" aria-expanded="false"><i class="fas fa-chart-area fa-fw"
                                aria-hidden="true"></i><span class="hide-menu">Dashboard</span></a></li>
                                <li class="sidebar-item"> <a class="sidebar-link waves-effect waves-dark sidebar-link"
                                    href="index.html" aria-expanded="false"><i class="fas fa-chart-area fa-fw"
                                        aria-hidden="true"></i><span class="hide-menu">Add Personal Expense</span></a></li>
                                        <li class="sidebar-item"> <a class="sidebar-link waves-effect waves-dark sidebar-link"
                                            href="index.html" aria-expanded="false"><i class="fas fa-chart-area fa-fw"
                                                aria-hidden="true"></i><span class="hide-menu">View Expenses</span></a></li>
                                                <li class="sidebar-item"> <a class="sidebar-link waves-effect waves-dark sidebar-link"
                                                    href="index.html" aria-expanded="false"><i class="fas fa-chart-area fa-fw"
                                                        aria-hidden="true"></i><span class="hide-menu">View Bills</span></a></li>
                    <li class="sidebar-item"> <a class="sidebar-link waves-effect waves-dark sidebar-link"
                            href="index.html" aria-expanded="false"><i class="fas fa-chart-area fa-fw"
                                aria-hidden="true"></i><span class="hide-menu">Logout</span></a></li>     
                    </ul>

                </nav>
               
            </div>
           
        </aside>
        
        <div class="page-wrapper" style="min-height: 250px;">
    
            <div class="container-fluid">
                <h1> Add Expense </h1> 
                <div class="row">
                    <div class="col-lg-8 col-xlg-9 col-md-12">
                        <div class="card">
                            <div class="card-body">
                                <form action="/insertPersonal" method="POST">

                                    <div class="form-group mb-4">
                                       
                                        
                                        <label for="example-email" class="col-md-12 p-0" for="pname" >Item Name</label>
                                        <div class="col-md-12 border-bottom p-0">
                                            <input type="text" placeholder="69" class="form-control p-0 border-0" name="pname" id="example-email">
                                        </div>
                                    </div>

                                    <div class="form-group mb-4">
                                        <label for="example-email" class="col-md-12 p-0" for="pprice">Price</label>
                                        <div class="col-md-12 border-bottom p-0">
                                            <input type="text" placeholder="69" class="form-control p-0 border-0" name="pprice" id="example-email">
                                        </div>
                                    </div>

                                    <div class="form-group mb-4">
                                        <label for="example-email" class="col-md-12 p-0" for="pquantity">Quantity</label>
                                        <div class="col-md-12 border-bottom p-0">
                                            <input type="text" placeholder="69" class="form-control p-0 border-0" name="pquantity" id="example-email">
                                        </div>
                                    </div>

                                    <div class="form-group mb-4">
                                        <label for="example-email" class="col-md-12 p-0" for="ptotal">Total</label>
                                        <div class="col-md-12 border-bottom p-0">
                                            <input type="text" placeholder="69" class="form-control p-0 border-0" name="ptotal" id="example-email">
                                        </div>
                                    </div>
                                    
                                   
                                    </div>
                                    <div class="form-group mb-4">
                                        <div class="col-sm-12">
                                            <button class="btn btn-success text-black-50">Add Expense</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>

                    
                </div>

                
              
            </div>
            
           
            <footer class="footer text-center"> 2020 Â© iBill <a>
                    href="https://www.wrappixel.com/">iBill.com</a>
            </footer>
           
        </div>
      
